{
    "name": "CaseDownloadHelper",
    "description": "某网案例下载助手",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
        "js": [
            "js/content_script.js"
        ],
        "matches": [
            "https://case.cdgdc.edu.cn/case/*",
            "https://case.cdgdc.edu.cn/member/case/*"
        ],
        "run_at": "document_end"
    }],
    "action": {
        "default_popup": "html/popup.html",
        "default_icon": "img/icon-19.png"
    },
    "icons": {
        "128": "img/icon-128.png",
        "16": "img/icon-16.png",
        "19": "img/icon-19.png",
        "24": "img/icon-19.png",
        "32": "img/icon-19.png",
        "48": "img/icon-48.png",
        "64": "img/icon-64.png"
    }
}