{
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "2021.11.18",
    "manifest_version": 3,
    "default_locale": "en_US",
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
        "js": [
            "js/content_script.js"
        ],
        "matches": [
            "https://case.cdgdc.edu.cn/case/*",
            "https://case.cdgdc.edu.cn/member/case/*"
        ],
        "run_at": "document_end"
    }],
    "action": {
        "default_popup": "html/popup.html",
        "default_icon": "img/icon-16.png"
    },
    "icons": {
        "16": "img/icon-16.png",
        "19": "img/icon-19.png",
        "24": "img/icon-24.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "64": "img/icon-64.png",
        "128": "img/icon-128.png"
    }
}